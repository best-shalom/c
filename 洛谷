134.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,a[100],b[100],k=0;
				cin>>n;
				for(int i=0;i<n;i++)
				cin>>a[i];
				sort(a,a+n);                           //排序 
				for(int i=0;i<n;i++)                 //去重 
				{
					if(a[i]!=a[i+1])
					{
						b[k]=a[i];
						k++;                            //将a数组中不相同的部分存到b数组中  
					}
				}
				cout<<k<<endl;
				for(int i=0;i<k;i++)
				cout<<b[i]<<" ";
				return 0;
			}






135.			#include <bits/stdc++.h>              //nice！ 
			using namespace std;
			int main()
			{
				int n,t,a[1000]={0};                 //初始化a数组为0 
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>t;
					a[t]=1;                        //对于相应的学号t，a数组值变为1 
				}
				int s=0;
				for(int i=0;i<1000;i++)
				{
					if(a[i]==1)
					s++;                           //表明存在且不重复的学号数目（对于重复学号,a[学号]==1也只出现一次） 
				}
				cout<<s<<endl;
				for(int i=0;i<1000;i++)
				{
					if(a[i]==1)
					cout<<i<<" ";                  //输出存在的学号 
				}
				return 0;
			}





136.			#include <bits/stdc++.h>
			using namespace std;
			struct student{
				int id;
				int a;
				int b;
				int c;
				int s;
			}name[300];
			int main()
			{
				int n;
				cin>>n;
				for(int i=1;i<=n;i++)
				{
					cin>>name[i].a>>name[i].b>>name[i].c;
					name[i].s=name[i].a+name[i].b+name[i].c;     
					name[i].id=i;                                  //总分，i为对应学号 
				}
				int p=5,max=1;                                    //输出5次
				while(p--)
				{
					for(int i=2;i<=n;i++)
					{   
					    //判断条件是否符合最大值 
						if((name[i].s>name[max].s)||(name[i].s==name[max].s&&name[i].a>name[max].a)||(name[i].s==name[max].s&&name[i].a==name[max].a&&name[i].id<name[max].id))
						{
							max=i;
						}
					}
					cout<<name[max].id<<" "<<name[max].s<<endl;
					name[max].s=0;                                //将已经输出的最大值排除掉
					max=1;                                        //进行下一次循环判断 
				}
				return 0;
			}





137.			#include <bits/stdc++.h>
