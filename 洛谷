                        using namespace std;
                        int main()
                        {
                          int n,s=0,S=0,a[11],m;
                          for (int i=1;i<=12;i++)
                          {
                            cin>>a[i];
                            s+=300-a[i];
                            if(s<0)
                            {
                            cout<<'-'<<i;
                            return 0;
                            }
                            while (s>=100)
                            {
                              S=S+100;
                              s=s-100;
                            }
                           } 
                           m=S*6/5+s;
                           cout<<m;
                           return 0;
                         } 





47.                           #include <bits/stdc++.h>
                              using namespace std;
                              int main()
                              {
                                int n,a[n],j=0;
                                cin>>n;
                                for (int i=1;i<=n;i++)
                                {
                                  cin>>a[i];
                                  if (i==1)
                                  cout<<0<<" ";
                                  else
                                  {
                                    for (int k=1;k<i;k++)
                                    {
                                      if (a[i]>a[k])
                                      j++;	
                                    }
                                  cout<<j<<" ";
                                  j=0;
                                  }
                                }
                                return 0;
                              }




