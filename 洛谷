			struct s{
				string x;                         //票数超出long long使用字符串储存
				int len;
				int id;
			}name[20];
			int main()
			{
				int n;
				cin>>n;
				for(int i=1;i<=n;i++)
				{
					cin>>name[i].x;
					name[i].len=name[i].x.size();
					name[i].id=i;
				}
				int max=1;
				for(int i=2;i<=n;i++)
				{
					if((name[i].len>name[max].len)||(name[i].len==name[max].len&&name[i].x>name[max].x))
					max=i;
				}
				cout<<name[max].id<<'\n'<<name[max].x;
				return 0;
			}




