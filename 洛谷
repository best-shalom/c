157.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int a,b,n,j=0,maxi=0;
				for(int i=1;i<=7;i++)
				{
					cin>>a>>b;
					n=a+b-8;                     //用n表示每天的不高兴值 
					if(n>maxi)                   //如果两天相等，输出的仍然是靠前的一天 
					{
						maxi=n;
						j=i;
					}
					n=0;                         //第二天，刷新 
				} 
				cout<<j;                         //如果每天都开心，结果会为0 
				return 0;
			}



158.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{ 
				int n,m=0,i,s=0;                           //m表示上一个月剩下的钱，第一个月为0 
				for(i=1;i<=12;i++)                     //十二个月分别判断 
				{
					cin>>n;
					m+=300;
					if(m<n)                      //预算不够的情况，直接结束输出月份 
					{
						cout<<"-"<<i;
						return 0;
					}
					else
					{
						while(m-n>=100)
						{
							m-=100;              //存到妈妈那里
							s+=100; 
						}                        //一直到没有多余的整百
						m-=n;                    //花掉这一个月的钱，进入下一个月 
					}
				}
				cout<<double(s)*1.2+m;           //取得整百的钱，还有自己剩下的钱m 
				return 0;
			}



159.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,a,b,m[3];
				cin>>n;
				int i=3;
				while(i--)                        //三次循环判断每种方式的价格 
				{
					cin>>a>>b;
					if(n%a==0)                    //得到该种方式的价格 
					m[i]=(n/a)*b;                 
					else
					m[i]=(n/a+1)*b;         
				}
				sort(m,m+3);                       //判断最少的价格 
				cout<<m[0];                        
				return 0;
			 } 



