15.                                     #include <iostream>
                                        using namespace std;
                                        int main()
                                        {
                                            float m,h,BIM;
                                            cin>>m>>h;
                                            BIM=m/h/h;
                                            if (BIM<18.5)
                                            cout<<"Underweight";
                                            else if (BIM>=24)
                                            cout<<BIM<<'\n'<<"Overweight";
                                            else
                                            cout<<"Normal";
                                            return 0;
                                        }




16.                                         #include <iostream>
                                            #include <bits/stdc++.h>
                                            using namespace std;
                                            int main()
                                            {
                                                int a,b,c,t;
                                                cin>>a>>b>>c;
                                                if (a>b) swap(a,b);
                                                if (a>c) swap(a,c);
                                                if (b>c) swap(b,c);
                                                cout<<a<<' '<<b<<' '<<c;
                                                return 0;
                                            }



