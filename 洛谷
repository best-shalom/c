161.			#include <iostream>
			using namespace std;
			int l[100000];                         //初始化表示本来的树木
			int main()
			{
				int n,m,a,b,s=0;
				cin >>m>> n;
				for (int i = 0; i < n; i++)               //n个循环输入要除去的树木
				{
					cin >> a >> b;
					for (int j = a; j <= b; j++)
					{
						l[j] = 1;                      //包括端点的树木被移走,用1表示
					}
				}
				for (int i = 0; i < m+1; i++)            //判断树木剩余(马路长m,但是有m+1颗树)
				{
					if (l[i] == 0)
					{
						s++;
					}
				}
				cout << s;
				return 0;
			}




162.			#include <iostream>
			using namespace std;
			int main()
			{
				int a[100],b;
				for(int i=0;i<100;i++)
				{
					cin >> a[i];
					if (a[i] == 0)
					{
						b = i+1;                 //表示输入数字的个数
						break;
					}
				}
				for (int i = b - 2; i >= 0; i--)         //除去最后一位的0
				{
					cout << a[i] << " ";
				}
				return 0;
			}




163.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,a[101],s=0,b[101];
				cin >> n;
				for (int i = 0; i < n; i++)               //n次循环输入数据
				{
					cin >> a[i];
					b[i] = 1;
				}
				sort(a, a + n);
				for (int i = 0; i < n; i++)
				{
					for (int j = 0; j < n-1 ; j++)
					{
						for (int k = j + 1; k < n; k++)
						{
							if (a[j] + a[k] == a[i]&&b[i]==1)         //注意去重:如果a+b==c+d==e,根据题意此时只能算一次e
							{
								s++;                                  //求和
								b[i] = 0;                             //b[i]这个数可以跳过了
							}
						}
					}
				}
				cout << s;
				return 0;
			}




164.			#include <bits/stdc++.h>
			using namespace std;
			int t[1005][1005];                               //初始化
			int main()
			{
				int n, m, k, a, b=0;
				cin >> n >> m >> k;
				for (int i = 0; i < n; i++)                  //n个人的训练
				{
					for (int j = 1; j<=m; j++)               //m套题的训练天数
					{
						cin >> a;                            
						t[a][j]++;                            //a表示第a天训练的人,j表示此人训练的套数
					}
				}
				for (int i = 1; i <=k; i++)                   //循环判断老师每天的准备量
				{
					for (int j = 1; j <=m; j++)                //对每位同学做的套数(第j套)做判断
					{
						if (t[i][j])                           //在同一天做同一套题的人不止一个,老师的准备量均为1
						{
							b ++;
						}
					}
					cout << b << " ";
					b = 0;
				}
				return 0;
			}






165.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				string a;
				getline(cin, a);                       //注意含空格字符串的输入及求长度
				int n = a.size(),t=0;
				for (int i = 0; i < n; i++)
				{
					if (a[i] != ' ')
					{
						t++;
					}
				}
				cout << t;
				return 0;
			}










166.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				char a[13];
				int s = 0, n, t = 0;
				for (int i = 0; i < 13; i++)
				{
					cin >> a[i];
				}
				for (int i = 0; i < 11; i++)
				{
					if (a[i] >= '0' && a[i] <= '9')                        //判断数字
					{
						t++;
						s += (a[i] - '0') * t;                             //!!!将字符数字改成整型数字并按照识别码求法计算
					}
				}
				n = s % 11;
				if ((n == a[12] - '0')||((n==10)&&(a[12]=='X')))
				{
					cout << "Right";
				}
				else
				{
					a[12] = n + '0';
					if (n == 10)
					{
						a[12] = 'X';
					}
					for (int i = 0; i < 13; i++)
					{
						cout << a[i];
					}
				}
				return 0;
			}



167.			#include <bits/stdc++.h>
			using namespace std;
			void word(string a, int m, int n,int t=0, string b[100000])             //定义分词函数,其中m表示划分的初始位置,t表示单词个数
			{
				int i;
				for (i = m; i < n; i++)               //按照空格把文章a分成单词组b
				{
					if (a[i] == ' ')
					{
						b[t] = a.substr(0, i-m);       //从a[0]处拷贝i-m个字符到b中
						t++;
					}
				}
				if (m == n)
					return;
				else
					word(a, i + 1, n, t, b[100000]);    //递归....?(虽然现在还不会用哈哈哈)

			}
			int main()
			{
				string a, b;
				int t = 0, s = 0, l = 0;
				string c[100000];
				cin >> a;                            //输入要查找的单词
				getline(cin, b);
				int m = a.size();
				int n = b.size();
				for (int i = 0; i < m; i++)           //将所有大写字母换为小写(查找单词)
				{
					if (a[i] >= 'A' && a[i] <= 'Z')
					{
						a[i] += 26;
					}
				}
				for (int i = 0; i < n; i++)           //将所有大写字母换为小写(输入文章)
				{
					if (b[i] >= 'A' && b[i] <= 'Z')
					{
						b[i] += 26;
					}
				}
				word(b, 0, n, t, c[100000]);
				for (int i = 0; i < t; i++)           //查找单词
				{
					if (strcmp(c[i], a) == 0)      
					{
						s++;                          //额,还要我判断初始位置...
						if (s == 1)                   //储存第一个符合条件单词的头位置
						{
							int L = l;
						}
					}
					l += c[i].size()+1;               //表示下一个单词的头位置
				}
				if (s == 0)
				{
					cout << -1;
				}
				else
				{
					cout << s << " " << L;
				}
				return 0;
			}





168.			#include <bits/stdc++.h>
			using namespace std;
			int Change(int n)
			{
				int a[8] = {0}, i = 0;                                        //保证八位数判断回文
				while (n)
				{
					a[i] = n % 10;                                      //a[i]中储存各位数字
					n = n / 10;
					i++;
				}
				for (int i = 7; i >= 0; i--)
				{
					cout << a[i];
				}
				cout << endl;
				int j = 0, k = 7;
				while(j<k)
				{
					if (a[j] != a[k])
					{
						return 0;
					}
					j++, k--;
				}
				return 1;                                              //1表示回文,2表示非回文
			}
			int Time(int n)
			{
				int a[8] = {0}, i = 7;                                        //保证八位数对日期年份进行判断
				while (n)
				{
					a[i] = n % 10;                                      //a[i]中储存各位数字
					n = n / 10;
					i--;
				}
				int Y = a[0] * 1000 + a[1] * 100 + a[2] * 10 + a[3], M = a[4] * 10 + a[5], D = a[6] * 10 + a[7];        //分别表示年月日
				//如果年月日都相互匹配,返回1,否则返回0
				//1.判断大小月份
				if (M % 2 == 0 && M != 2)                                         //二月独立出来判断
				{
					if (D != 30)
					{
						return 0;
					}
				}
				else if (M % 2 == 1 )                                         
				{
					if (D != 31)
					{
						return 0;
					}
				}
				//2.判断二月
				else if (M == 2)
				{
					if ((Y % 4 == 0 && Y % 100 != 0) || Y % 400 == 0)             //闰年
					{
						if (D != 29)
						{
							return 0;
						}
					}
					else
					{
						if (D != 28)
						{
							return 0;
						}
					}
				}
					return 1;
			}
			int main()
			{
				int a, b, s = 0;
				cin >> a >> b;
				for (int i = a; i <= b; i++)
				{
					if (Change(i) == 1&&Time(i)==1)                     //用change函数判断是否回文,Time函数判断日期是否存在
					{
						s++;
					}
				}
				cout << s;
				return 0;
			}





169.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n;
				cin >> n;
				string a[20];
				for (int i = 0; i < n; i++)
				{
					cin >> a[i];
				}
				for (int i = 0; i < n-1; i++)
				{
					for (int j = i + 1; j < n; j++)
					{
						if (a[i] + a[j] < a[j] + a[i])
							swap(a[i], a[j]);                         //这样交换之后,从a[0]开始输出即为最大值
					}
				}
				for (int i = 0; i < n; i++)
				{
					cout << a[i];
				}
				return 0;
			}




170.			#include <bits/stdc++.h>
			using namespace std;
			int Time(int n)                                              //这里输入的是一个年份,需要判断的是与其相应的回文月,日是否合法
			{
				int a[4] , i = 3;                                        //保证八位数对日期年份进行判断
				while (n)
				{
					a[i] = n % 10;                                              //a[i]中储存各位数字
					n = n / 10;
					i--;
				}
				int  M = a[3] * 10 + a[2], D = a[1] * 10 + a[0];                  //分别表示年月日
				//如果年月日都相互匹配,返回1,否则返回0
				//1.判断大小月份
				if (M % 2 == 0 && M != 2)                                         //二月独立出来判断
				{
					if (D > 30)
					{
						return 0;
					}
				}
				else if (M % 2 == 1)
				{
					if (D > 31)
					{
						return 0;
					}
				}
				//2.判断二月
				else if (M == 2)
				{
					if ((n % 4 == 0 && n % 100 != 0) || n % 400 == 0)             //闰年
					{
						if (D > 29)
						{
							return 0;
						}
					}
					else
					{
						if (D > 28)
						{
							return 0;
						}
					}
				}
				return 1;
			}
			int main()
			{
				int n1, n2, m1, m2, a1[8], a2[8], i = 7, b, c, s = 0;
				cin >> n1 >> n2;
				m1 = n1;
				while (m1)                         //储存各位数(起始)
				{
					a1[i] = m1 % 10;
					m1 = m1 / 10;
					i--;
				}
				b = a1[0] * 1000 + a1[1] * 100 + a1[2] * 10 + a1[3];
				i = 7;
				m2 = n2;
				while (m2)                         //储存各位数(终止)
				{
					a2[i] = m2 % 10;
					m2 = m2/ 10;
					i--;
				}
				c = a2[0] * 1000 + a2[1] * 100 + a2[2] * 10 + a2[3];

				for (int j = b; j <= c; j++)
				{
					if (Time(i) == 1)                   //表示存在相应日月
					{
						s++;
					}
				}
				cout << s;
				return 0;
			}




171.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n, a;
				cin >> n;
				for (int i = n; i >= 0; i--)
				{
					cin >> a;
					if (i == n)
					{
						if (a == 0)
							continue;
						else if (a == 1)
							cout << "x" << "^" << i;
						else if (a == -1)
							cout <<"-"<< "x" << "^" << i;
						else
							cout << a << "x" << "^" << i;
					}
					else if (i == 1)
					{
						if (a == 0)
							continue;
						else if (a == 1)
							cout << "+" << "x";
						else if (a == -1)
							cout << "-" << "x";
						else if (a > 0)
							cout << "+" << a << "x";
						else
							cout << a << "x";
					}
					else if (i == 0)
					{
						if (a == 0)
							continue;
						else if (a > 0)
							cout << "+" << a;
						else
							cout << a;
					}
					else
					{
						if (a == 0)
							continue;
						else if (a == 1)
							cout <<"+"<< "x" << "^" << i;
						else if (a == -1)
							cout << "-" << "x" << "^" << i;
						else if (a > 0)
							cout << "+" << a << "x" << "^" << i;
						else
							cout << a << "x" << "^" << i;
					}
				}
				return 0;
			}




172.			#include <bits/stdc++.h>
			using namespace std;
			int Num(int k,int sum)
			{
				sum=0; 
				int len=sizeof(k);
				int a=1;
				while(a<=k/2)
				{
					sum+=Num(a,sum);
					a++;
					sum++; 
				}
				return sum;
			} 
			int main()
			{
				int k;
				cin>>k;
				cout<<Num(k,0)+1;                      //加上自己 
				return 0;
			}
