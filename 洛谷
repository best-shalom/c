140.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,a[1000],b[1000];
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>a[i];
				}
				for(int i=0;i<n-1;i++)
				{
					b[i]=a[i+1]-a[i];                  //b数组储存差值 
					if(b[i]<0)
					b[i]=-b[i];                        //取绝对值 
				}
				sort(b,b+n-1);
				for(int i=0;i<n-1;i++)
				{
					if(b[i]!=i+1)                      //判断是否在[1,n-1]上 
					{
						cout<<"Not jolly";
						return 0;cout<<"Not jolly";
					}
				}
				cout<<"Jolly";
				return 0;
			}



142.			#include <bits/stdc++.h>
			using namespace std;
			struct name{
				int id;
				int x;
			}a[5000];
			void xsort(name *p,int n)                                 //对含有n个数据组的结构体中，按照"分数"部分从大到小排序 
			{
				name temp;
				for(int i=0;i<n;i++)
				{
					for(int j=i+1;j<n;j++)
					{
						if(p[i].x<p[j].x)
						{
							temp=p[i];p[i]=p[j];p[j]=temp;
						}
						else if(p[i].x==p[j].x&&p[i].id>p[j].id)       //注意，还有学号的影响 
						{
							temp=p[i];p[i]=p[j];p[j]=temp;
						}
					}
				}
			}
			int main()
			{
				int m,n;
				cin>>n>>m;
				for(int i=0;i<n;i++)
				{
					cin>>a[i].id>>a[i].x;
				}
				xsort(a,n);                                            //按分数排序 
				int p=m*1.5,t=0;                                       //找到面试分数线 
				for(int i=0;i<n;i++)
				{
					if(a[i].x>=a[p].x)
					t++;                                               //得到可以参加面试的人数 
				} 
				cout<<a[p].x<<" "<<t<<endl;
				int i=0;
				while(t--)                                             //t次循环输出分数线上的面试者情况 
				{
					cout<<a[i].id<<" "<<a[i].x<<endl;                       //输出 
					i++;
				}
				return 0;
			}















143.			#include <bits/stdc++.h>
			using namespace std;
			struct name{
				int id;
				int x;
			}a[5000];
			void xsort(name *p,int n)                                 //对含有n个数据组的结构体中，按照"分数"部分从大到小排序 
			{
				name temp;
				for(int i=0;i<n;i++)
				{
					for(int j=i+1;j<n;j++)
					{
						if(p[i].x<p[j].x)
						{
							temp=p[i];p[i]=p[j];p[j]=temp;
						}
						else if(p[i].x==p[j].x&&p[i].id>p[j].id)       //注意，还有学号的影响 
						{
							temp=p[i];p[i]=p[j];p[j]=temp;
						}
					}
				}
			}
			int main()
			{
				int m,n;
				cin>>n>>m;
				for(int i=0;i<n;i++)
				{
					cin>>a[i].id>>a[i].x;
				}
				xsort(a,n);                                            //按分数排序 
				int p=m*1.5-1,t=0;                                       //找到面试分数线 (录取m*1.5个人，分数线对应序号为m*1.5-1) 
				for(int i=0;i<n;i++)
				{
					if(a[i].x>=a[p].x)
					t++;                                               //得到可以参加面试的人数 
				} 
				cout<<a[p].x<<" "<<t<<endl;
				int i=0;
				while(t--)                                             //t次循环输出分数线上的面试者情况 
				{
					cout<<a[i].id<<" "<<a[i].x<<endl;                       //输出 
					i++;
				}
				return 0;
			}









144.			#include <bits/stdc++.h>
			using namespace std;
			struct place{
				double x;
				double y;
				double z;
			}t[50000];
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>t[i].x>>t[i].y>>t[i].z;
				}
				place temp;
				for(int i=0;i<n-1;i++)                        //按高度排序 
				{
					for(int j=i+1;j<n;j++)
					{
						if(t[i].z<t[j].z)
						{
							temp=t[i];
							t[i]=t[j];
							t[j]=temp;
						}
					}
				}
				double s=0;
				for(int i=0;i<n-1;i++)
				{
					s+=sqrt((t[i].x-t[i+1].x)*(t[i].x-t[i+1].x)+(t[i].y-t[i+1].y)*(t[i].y-t[i+1].y)+(t[i].z-t[i+1].z)*(t[i].z-t[i+1].z));       //从低到高加距离 
				}
				printf("%.3lf",s);
				return 0;
			}





145.			#include <bits/stdc++.h>
			using namespace std;
			struct pla{
				double x;
				double y;
				double z;
			}t[50000];
			double dis(pla a,pla b)                //算距离函数 
			{
				return sqrt((a.x-b.x)*(a.x-b.x)+(a.y-b.y)*(a.y-b.y)+(a.z-b.z)*(a.z-b.z));
			}
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>t[i].x>>t[i].y>>t[i].z;
				}
				pla temp;
				for(int i=0;i<n-1;i++)                        //按高度排序 
				{
					for(int j=i+1;j<n;j++)
					{
						if(t[i].z<t[j].z)
						{
							temp=t[i];
							t[i]=t[j];
							t[j]=temp;
						}
					}
				}
				double s=0;
				for(int i=0;i<n-1;i++)
				{
					s+=dis(t[i],t[i+1]);                   //从低到高加距离 
				}
				printf("%.3lf",s);
				return 0;
			}




146.			#include <bits/stdc++.h>
			using namespace std;
			struct pl{
				double x;
				double y;
				double z;
			}t[50000];
			bool cmp(pl a,pl b)
			{
				return a.z<b.z;
			}
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>t[i].x>>t[i].y>>t[i].z;
				}
				sort(t,t+n,cmp);
				double s=0;
				for(int i=0;i<n-1;i++)
				{
					s+=sqrt((t[i].x-t[i+1].x)*(t[i].x-t[i+1].x)+(t[i].y-t[i+1].y)*(t[i].y-t[i+1].y)+(t[i].z-t[i+1].z)*(t[i].z-t[i+1].z));                   //从低到高加距离 
				}
				printf("%.3lf",s);
				return 0;
			}





147.			#include <bits/stdc++.h>
			using namespace std;
			struct student{
				string id;
				int a,b,c;
			}t[100];
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>t[i].id>>t[i].a>>t[i].b>>t[i].c;
				}
				student temp;
				for(int i=0;i<n-1;i++)
				{
					for(int j=i+1;j<n;j++)
					{
						if((t[i].a>t[j].a)||((t[i].a==t[j].a)&&(t[i].b>t[j].b))||((t[i].a==t[j].a)&&(t[i].b==t[j].b)&&(t[i].c>t[j].c))||((t[i].a==t[j].a)&&(t[i].b==t[j].b)&&(t[i].c==t[j].c)))
						{
							temp=t[i];t[i]=t[j];t[j]=temp;             //按年龄排序 
						}
					}
				}
				for(int i=0;i<n;i++)
				{
					cout<<t[i].id<<endl;
				}
				return 0;
			}




148.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				string a[20];
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				cin>>a[i];
				for(int i=0;i<n-1;i++)
				{
					for(int j=i+1;j<n;j++)
					{
						if(a[i]+a[j]<a[j]+a[i])                           //神奇的字符串，可以直接比较大小 
						swap(a[i],a[j]);
					}
				}
				for(int i=0;i<n;i++)
				cout<<a[i];
				return 0;
			}




149.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int m,n,a,b;
				cin>>n>>m;
				a=n;b=m;
				long long s1,s2;
				while(a>0&&b>0)
				{
					s1+=a*b;                            //其中正方形个数为n*m+(n-1)*(m-1)+..... 
					a--;b--;
				}
				s2=(n*(n+1)/2)*(m*(m+1)/2)-s1;
				cout<<s1<<" "<<s2;
			}




