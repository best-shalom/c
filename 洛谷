43.                               #include <iostream>
                                  using namespace std;
                                  int main()
                                  {
                                    int n,k=1,t=1;
                                    cin>>n;
                                    for (int i=1;i<=n*n;i++)
                                    {
                                      if(k<10)
                                      cout<<0<<k;
                                      else
                                      cout<<k;
                                      k++;
                                      t++;
                                      if(t>n) 
                                      {
                                        cout<<'\n';
                                        t=1;
                                      }
                                    }
                                    cout<<'\n';
                                    k=1;
                                    for (int i=1;i<=n;i++)
                                    {
                                      for (int j=1;j<=n-i;j++)
                                      {
                                        cout<<"  ";
                                      }
                                      while(k<=i*(i+1)/2)
                                      {
                                        if(k<10)
                                        cout<<0<<k;
                                        else
                                        cout<<k;
                                        k++;	
                                        }
                                      cout<<'\n';
                                      }
                                      return 0;
                                  }




44.                         #include <iostream>
                            #include <bits/stdc++.h>
                            using namespace std;
                            int main()
                            {
                              int maxi=0,mini=10,a[1000],n,s=0;
                              cin>>n;
                              for (int i=1;i<=n;i++)
                              {
                                cin>>a[i];
                                maxi=max(a[i],maxi);
                                mini=min(a[i],mini);
                                s=s+a[i];
                              }
                              s=s-maxi-mini;
                              double d=(double)s/(n-2);
                              printf ("%.2lf",d);
                              return 0;
                            }




