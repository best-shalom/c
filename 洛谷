69.             #include <bits/stdc++.h>
		using namespace std;
		int main()
		{
			char a[10];
			int s=0,n;
			fgets(a,10,stdin);
			n=strlen(a);
			for (int i=0;i<n;i++)
			{
				if(a[i]>='a'&&a[i]<='z')
				s++;
				if(a[i]>='A'&&a[i]<='Z')
				s++;
				if(a[i]>='0'&&a[i]<='9')
				s++;
			}
			cout<<s;
			return 0;
		 } 




70.                     #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int s1,s2,s3,t,n=1,s[20000],maxi=0,h=0;
				cin>>s1>>s2>>s3;
					for (int i=1;i<=s1;i++)
					{
						for (int j=1;j<=s2;j++)
						{
							for (int k=1;k<=s3;k++)
							{
								s[h]=i+j+k;
								h++;
							}
						}
				}
				sort(s,s+s1*s2*s3);
				for (int i=0;i<s1*s2*s3;i++)
				{
					if (s[i]==s[i+1])
					{
						n++;
						if(n>maxi)
						t=s[i];
						maxi=max(n,maxi);
					}
					else
					n=1;
				}
				cout<<t;
				return 0;
			}






71.（错的）              #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,h=1,t[2000000],m[2000000000],l[2000000000000];
				cin>>n;
				double a[5000];
				for (int i=0;;i++)
				l[i]=0;                                   //最开始所有的灯都是关的，用0表示关，1表示开 
				for (int i=0;i<n;i++)
				{
					cin>>a[i]>>t[i];
					m[i]=a[i]*t[i];                      //将a*t代表的灯用m数组储存 
					for (int k=0;;k++)                   //遍历数组
					{
						if (l[m[i]]==0)                  //对第m[i]个路灯进行开关操作 
						l[m[i]]=1;
						if (l[m[i]]==1)
						l[m[i]]=0;
					 } 
				}
				for (int i=0;;i++)                        //遍历数组，如果出现开着的灯即跳出，输出结果（数据保证唯一灯亮）
				{
					if (l[i]==1)
					break;
					h++;
				 } 
				 cout<<h;
				 return 0;
			}





72.                     #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,s=0,a;
				cin>>n;
				for (int i=0;s<n*n;i++)
				{
					cin>>a;                                      //不用数组储存，每输入一个数就直接打印对应的序列 
					for(int k=a;k>=1;k--)
					{
						if(i%2==1)
						cout<<1;                                 //直接把奇数的情况输出，其他的都是输出0 
						else
						cout<<0;
						s++;                                    //判断s是否为n的倍数，以确定是否换行
						if(s%n==0)
						cout<<'\n'; 
					 } 
				} 
				return 0;
			}




73.                     #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				string str;
				int s=0,n;
				getline(cin,str);
				n=str.size();
				for(int i=0;i<n;i++)
				{
					if(str[i]=='a'||str[i]=='d'||str[i]=='g'||str[i]=='j'||str[i]=='m'||str[i]=='p'||str[i]=='t'||str[i]=='w'||str[i]==' ')
					s+=1;
					if(str[i]=='b'||str[i]=='e'||str[i]=='h'||str[i]=='k'||str[i]=='n'||str[i]=='q'||str[i]=='u'||str[i]=='x')	
					s+=2;
					if(str[i]=='c'||str[i]=='f'||str[i]=='i'||str[i]=='l'||str[i]=='o'||str[i]=='r'||str[i]=='v'||str[i]=='y')
					s+=3;
					if(str[i]=='s'||str[i]=='z')
					s+=4;
				}
				cout<<s;
				return 0;
			}




74.                     //分析题目可知：输入时的情况一共有四种：VV,KK,VK,KV.
			//其中，VK符合条件，VV和KK可以通过改一个字母，KV无效. 
			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				string a;
				int n,s=0;
				cin>>n;                               //计算输入字符串的长度的方法：函数名.size()  or  strlen(函数名)
				cin>>a;
				for (int i=0;i<n-1;i++)               //只用循环到n-1，因为在比较i和i+1 
				{
					if(a[i]=='V'&&a[i+1]=='K')        //即判断输入VK的情况 
					{
						s++;
						a[i]='v';
						a[i+1]='k';                  //将已经使用过的字符变成v,k,防止对后面的判断造成影响 
					}
				}
				for (int i=0;i<n-1;i++)                 //再次循环判断 
				{
					if(a[i]==a[i+1])                  // 判断字符相同的情况，即KK和VV
					{
						s++;
						cout<<s;
						return 0;                        //出现第一个相等时就退出，因为只有一次修改字符的机会 
					} 
				}
				cout<<s;
				return 0;
			}




75.                     #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int boy=0,girl=0,n;
				string a;
				cin>>a;
				n=a.length();
				for (int i=0;i<n-2;i++)
				{
					if(a[i]=='b'||a[i+1]=='o'||a[i+2]=='y')
					boy++;                                            //即判断是否为连续的b,o,y或者是被覆盖的单独字母
					if(a[i]=='g'||a[i+1]=='i'||a[i+2]=='r'||a[i+3]=='l')
					girl++;                                           //girl同理 
				}
				cout<<boy<<'\n'<<girl;
				return 0;
			}





76.                     #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				string a;
				map<string,int>q;
				int b[10],k=0,m,j=0;
				 q["one"]=1;q["two"]=2;q["three"]=3;q["four"]=4;q["five"]=5;q["six"]=6;q["seven"]=7;q["eight"]=8;q["nine"]=9;q["ten"]=10;
				 q["eleven"]=11;q["twelve"]=12;q["thirteen"]=13;q["fourteen"]=14;q["fifteen"]=15;q["sixteen"]=16;q["seventeen"]=17;q["eighteen"]=18;q["nineteen"]=19;q["twenty"]=20;
				 q["a"]=1;q["both"]=2;q["another"]=1;q["first"]=1;q["second"]=2;q["third"]=3;
				for (int i=0;i<6;i++)                             //含有6个单词的句子 
				{
					cin>>a;
					if(q[a])                                       //对于含数字的字符串进行平方取模操作（排除为结果0的情况） 
					{
						m=q[a]*q[a]%100;
						if(m!=0)
						{
							b[j]=m;
							j++;
							k++;                                        //用k统计数字数 
						}

					}
				}
				sort(b,b+k);                                        //排序
				cout<<b[0]; 
				for (int i=1;i<k;i++)                               //对后续数字不足两位数前面补0
				{
					if(b[i]<10)
					cout<<0;
					cout<<b[i];
				 } 
				 return 0;
			}





