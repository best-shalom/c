127.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,t,mini=1000000,a[100000];
				cin>>n;
				for(int i=0;i<n;i++)
					cin>>a[i];
				for(int i=0;i<n;i++)
				{
					int k=i;                          //表示初始位置
					for(int j=i+1;j<n;j++)
					{
						if(a[j]<a[k])                 //如果最小值不在初始位置，则将位置改变
						k=j; 
					}
					if(k!=i)                          //如果最后位置不在初始值上，表明最小值不在初始位置，交换最小值
					{
						t=a[k];
						a[k]=a[i];
						a[i]=t;
					} 
				}
				for(int i=0;i<n;i++)
				cout<<a[i]<<" ";
			}





128.			#include <bits/stdc++.h>
			using namespace std;
			int a[100000]; 
			void quicksort(int left,int right)                       //输入头尾位置,快速排序使得整数数组a的[p,q]部分有序
			{
				if(left>right)                                           //递归终止 
			    return; 
				int i=left,j=right;
			//分别从初始序列两端开始“探测”。先从右往左找一个小于temp的数，再从左往右找一个大于temp的数，然后交换他们。
				int temp=a[left],t;                                    //取基准为左端点
				while(i!=j)                                       //在ij没有相遇的时候 
				{
			//哨兵j一步一步地向左挪动（即j--），直到找到一个小于temp的数停下来。接下来哨兵i再一步一步向右挪动（即i++），直到找到一个数大于temp的数停下来。
					while(a[j]>=temp&&i<j)                              //右侧 
					j--;
					while(a[i]<=temp&&i<j)
					i++;
					//一直到两侧不符合条件，交换 
					if(i<j) 
			       { 
					t=a[i]; 
					a[i]=a[j]; 
					a[j]=t; 
			       } 
			   }
			    a[left]=a[i];                                                    //最终将基准数归位,交换数值 
			    a[i]=temp; 
				quicksort(left,i-1);//继续处理左边的，这里是一个递归的过程 
			    quicksort(i+1,right);//继续处理右边的 ，这里是一个递归的过程 
			}
			int main()
			{

				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				cin>>a[i];
				quicksort(0,n-1);                                    //快排算法 
				for(int i=0;i<n;i++)
				cout<<a[i]<<" ";
			}



129.			#include <bits/stdc++.h>
			using namespace std;
			int a[100000];
			void qs(int l,int r)
			{
				int temp=a[(l+r)/2];                  //取中间为基准
				int i=l,j=r;
				while(i<=j) 
				{
					while(a[i]<temp)
					i++;                             //符合条件时
					while(a[j]>temp)
					j--;
					if(i<=j)                         //说明ij在中途停下了，即有数不符合条件，开始交换 
					{
						swap(a[i],a[j]); 
						i++;j--;
					}
				}
				if(l<j) qs(l,j);                  //递归搜索左半部分
			    if(i<r) qs(i,r);					//递归搜索右半部分
			}
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				cin>>a[i];
				qs(0,n-1);
				for(int i=0;i<n;i++)
				cout<<a[i]<<" ";
				return 0;
			}





130.			#include <bits/stdc++.h>
			using namespace std;
			int a[100000];
			void qs(int l,int r)
			{
				int temp=a[(l+r)/2];                //基准 
				int i=l,j=r;
				while(i<=j)
				{
					while(a[j]>temp)
					j--;
					while(a[i]<temp)
					i++;
					if(i<=j)
					{
						swap(a[i],a[j]);
						i++;j--;
					}
				}
				if(l<j)
				qs(l,j);
				if(i<r)
				qs(i,r);
			}
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				cin>>a[i];
				qs(0,n-1);
				for(int i=0;i<n;i++)
				cout<<a[i]<<" ";
				return 0;
			}
			
			
			
			
			
			
			
