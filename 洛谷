50.                   #include <bits/stdc++.h>
                      using namespace std;
                      int z(int n)     //定义一个函数判断是否为质数,以返回值0,1来判断 
                      {
                        if (n==1)
                        return 0;
                        if (n==2)
                        return 1;
                        if (n%2==0)
                        return 0;
                        for (int i=2;i<n;i++)
                        {
                          if (n%i==0)
                          return 0;
                        }
                        return 1;
                       } 
                      int main()
                      {
                        int n,s=0,k=0;
                        cin>>n;
                        for (int i=2;s+i<=n;i++)       //判断质数口袋是否超载 
                        {
                          if(z(i)==1)
                          {
                            s+=i;
                            cout<<i<<'\n';
                            k++;
                          }
                        }
                        cout<<k;
                        return 0;
                      }



51.                       #include <bits/stdc++.h>
                          using namespace std;
                          int main()
                          {
                            long long n,a=1,b=1,c=0; 
                            cin>>n;
                            for(int i=3;i<=n;i++)      //斐波那契数列：Fn=Fn-1+Fn-2; 
                            {
                              c=a+b;
                              a=b;
                              b=c;
                            }
                            cout<<c<<".00";             //保留两位小数 
                            return 0;
                          }


52.                         #include <bits/stdc++.h>
                            using namespace std;
                            int main()
                            {
                              int n,i=1;
                              cin>>n;
                              for (int k=1;k<=n;k++)    //正方形矩阵,有n排 
                              {
                                while (i<=n*k&&i>n*(k-1))    //一排中的数据在此范围内 
                                {	if(i<10)
                                  cout<<0<<i;
                                  else 
                                  cout<<i;
                                  i++;
                                } 
                                cout<<'\n';
                              }
                              cout<<'\n'; 
                              i=1;
                              for (int k=1;k<=n;k++)           //n排 
                              {
                                for (int m=1;m<=n-k;m++)     //每排的空格数 
                              {
                                cout<<"  ";
                               } 

                               for (int j=1;j<=k;j++)          //每排输入的数据数 
                               {
                                  if(i<10)
                                  cout<<0<<i;
                                  else 
                                  cout<<i;
                                  i++;
                               }
                               cout<<'\n';
                              }
                              return 0;
                             } 



