54.                     #include <bits/stdc++.h>
                        using namespace std;
                        int main()
                        {
                          int n,a[10000],maxi=1,m=1;
                          cin>>n;
                          for (int i=0;i<n;i++)
                          {
                            cin>>a[i];
                          }
                          for (int i=0;i<n;i++)
                          {
                            if(a[i+1]==a[i]+1)
                            {
                              m++;
                              maxi=max(m,maxi);             //得到一个阶段中的连号，并比较得到最大连号 
                            }
                            else 
                            m=1;
                          }
                          cout<<maxi;
                          return 0;
                        }




55.              #include <bits/stdc++.h>
                 using namespace std;
		int main()
		{
			int n,a[1000],maxi=0,mini=10,s=0;
			float m;
			cin>>n;
			for (int i=0;i<n;i++)
			{
				cin>>a[i];
				maxi=max(maxi,a[i]);
				mini=min(mini,a[i]);
				s+=a[i];
			}
			s=s-maxi-mini;
			m=(float)s/(n-2);
			printf("%.2lf",m);
			return 0;
		}




56.                     #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,x,k=1;
				cin>>n;
				while(1)
				{
					for (x=100;x>0;x--)                    //列举K=1时，X从100到1是否能够成立，如果不存在，再考虑K=2时，以此类推，可得到K最小的解 
					{                                      //此处大括号可省，for运行唯一的if条件，无需大括号 
					if ((x*7+k*21)*52==n)
					{
						cout<<x<<'\n'<<k;
						return 0;
					}
					}
				k++;
				}
				return 0;
			 } 



57.                     #include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int l,m,p,q,a[10000],s=0;     //注意在0上也有树！ 
				cin>>l>>m;
				for (int i=0;i<=l;i++)        //对于L棵树，如果移除则为1，否则为0 
					a[i]=0;
				for (int i=1;i<=m;i++) 
				{
					cin>>p>>q;
					for (int i=p;i<=q;i++)  //在PQ范围内的数即为1 
						if (a[i]==0)
						a[i]=1;
				}
				for (int i=0;i<=l;i++)         //统计所有没有移除的树 
					if (a[i]==0)
					s++;
				cout<<s;
				return 0;
			}




58.                             #include <bits/stdc++.h>
				using namespace std;
				int main()
				{
					int n,a[1000],b[1000],c[1000],s[1000],k=0;
					cin>>n;
					for (int i=1;i<=n;i++)
					{
						cin>>a[i]>>b[i]>>c[i];
						s[i]=a[i]+b[i]+c[i];
					}
					for (int i=1;i<=n;i++)
					{
						for(int j=i+1;j<=n;j++)
						{
							if(abs(a[i]-a[j])<=5&&abs(b[i]-b[j])<=5&&abs(c[i]-c[j])<=5&&abs(s[i]-s[j])<=10)
							k++;
						}
					}
					cout<<k;
					return 0;
				}
				
				
				


59.                             #include <bits/stdc++.h>
				using namespace std;
				int main()
				{
					int w,x,h,a[21][21][21],q,x1,y1,z1,x2,y2,z2,m=0;          //初始化每个坐标为0，代表未被移走的方块 
					cin>>w>>x>>h>>q;
					while(q--)                //执行q次，直到while(0)结束 	 
					{
						cin>>x1>>y1>>z1>>x2>>y2>>z2;    
						for(int i=x1;i<=x2;i++)
						{
							for(int j=y1;j<=y2;j++)
							{
								for(int k=z1;k<=z2;k++)
								{
									a[i][j][k]=1;             //表示被移去的方块，值为1 
								}
							}
						}
					}
					for (int i=1;i<=w;i++)
					{
						for (int j=1;j<=x;j++)
						{
							for (int k=1;k<=h;k++)
							{
								if(a[i][j][k]==0)                   //统计未被移去的方块数 
								m++;
							}
						}
					}
					cout<<m;
					return 0;
				}






60                              #include <bits/stdc++.h>
using namespace std;
int main()
{
	int n,t[7],l[7],m=7,a=0,b=0,c=0,d=0,e=0,f=0,g=0;
	cin>>n;
	for (int i=0;i<7;i++)
	{
		cin>>t[i];                             //输入中奖彩票 
	}
	while(n--)                                 //对N张彩票进行分析 
	{
		m=7;
		for (int i=0;i<7;i++)
		{
			cin>>l[i];                           //输入买到的彩票号码 
			for(int j=0;j<7;j++)
			{
				if(t[j]==l[i])                    //将每一个号码与中奖号进行比较 
				m--;                                
			}
		}                                         //最终得到的m即为中奖等级 
	if(m==0)                                      //统计奖项 
	a++;
	if(m==1)
	b++;
	if(m==2)
	c++;
	if(m==3)
	d++;
	if(m==4)
	e++;
	if(m==5)
	f++;
	if(m==6)
	g++;
	}
	cout<<a<<" "<<b<<" "<<c<<" "<<d<<" "<<e<<" "<<f<<" "<<g;         //输出奖项 
	return 0;
} 






