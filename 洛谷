117.			#include <bits/stdc++.h>
			using namespace std;
			int num(int t)                 //判断质数 
			{
				if(t==2)
				return 1;
				int flag=1;
				for(int i=2;i<t;i++)
				{
					if(t%i==0)
					flag=0;
				}
				if(flag==1)
				return 1;
				else
				return 0;
			}
			void magic(int a)              //输出算式 
			{
				for(int i=2;i<a-1;i++)
				{
					if((num(i)==1)&&(num(a-i)==1))      //若均为质数 
					{
						cout<<a<<"="<<i<<"+"<<a-i<<endl;
						return;                       //输出算式后结束 
					}
				}
			}
			int main()
			{
				int n;
				cin>>n;
				for(int i=4;i<=n;i++)
				{
					if(i%2==0)             //判断偶数
					{
						magic(i);
					} 
				}
				return 0;
			}



118.			#include <bits/stdc++.h>
			using namespace std;
			bool check(int x)               //检查位数
			{
				if((1000 <= x && x <= 9999) || (100000 <= x && x <= 999999)) return 0;
				return 1;
			} 
			bool num(int t)                 //判断质数 
			{
				for(int i=3;i<=sqrt(t);i++)
				{
					if(t%i==0)
					return 0;		
				}
				return 1;
			}
			bool magic(int n)                  //判断回文 
			{
				int a[20],i=0,t=n;
				while(t)
				{
					a[i]=t%10;                //分别储存各个位数上的数字 (个十百。。)
					t=t/10;
					i++;
				}
				for(int j=0;j<=i/2;j++)
				{
					if(a[j]!=a[i-j-1])
					return 0;
				}
				return 1;
			}
			int main()
			{
				int a,b;
				cin>>a>>b;
				if(a%2==0)
				a++;                                 //从2的倍数+1开始搜索
				if(b>=9999999) 
				b=9999999;                          //最大的回文质数处理
				for(int i=a;i<=b;i+=2)                 //判断回文质数(判断奇数减少时间) 
				{
					if(check(i) == 0) continue;
					if(num(i) == 0) continue;
					if(magic(i) == 0) continue;
					printf("%d\n", i);                   //printf会比cout快很多
				}
				return 0;
			}



119.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				long long a,i=0,s=0;
				while(cin>>a)                   
				{
					s+=a;
					i++;
				} 
				//子集和：每个元素都被选中过2^(i-1)次
				for(int j=1;j<i;j++)          //i-1次 
				s*=2;
				cout<<s;
				return 0;
			}



