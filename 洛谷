111.			#include <bits/stdc++.h>
			using namespace std;
			int N(int n)
			{
				if(n==0)
				return 1;
				return n*N(n-1);            //n!=n*(n-1)!递归 
			}
			int main()
			{
				int n;
				cin>>n;
				cout<<N(n);
				return 0;
			}



112.			#include <bits/stdc++.h>
			using namespace std;
			int a[1025][1025];                  //2的十次方 
			void R(int t,int x,int y)             //赦免系统(递归),输入正方形边长，初始横纵坐标 
			{
				if(t==2)                       //边长为2时停止
				{
					a[x][y]=0;                 //顶角处为0
					return;	
				} 
				for(int i=x;i<x+t/2;i++)    //四分之一总面积变为0
				{
					for(int j=y;j<y+t/2;j++)
					a[i][j]=0;
				}
				R(t/2,x+t/2,y);
				R(t/2,x+t/2,y+t/2);
				R(t/2,x,y+t/2);		
			} 
			int main()
			{
				int n,s=1;
				cin>>n;
				for(int i=0;i<n;i++)
				s*=2;                  //2的n次方 (s表示矩阵边长)
				for(int i=0;i<s;i++)
				{
					for(int j=0;j<s;j++)
					a[i][j]=1;           //矩阵初始化 
				}
				R(s,0,0);
				for(int i=0;i<s;i++)
				{
					for(int j=0;j<s;j++)
					cout<<a[i][j]<<" ";
					cout<<'\n';            //输出矩阵 
				}
			}




113.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int n,i,t,maxi=0;
				cin>>n;
				int s[n],C[n],M[n],E[n];             //分别储存总分,语文，数学，英语成绩 
				string N[n];                           //储存名字
				for(i=0;i<n;i++)
				{
					cin>>N[i]>>C[i]>>M[i]>>E[i];
					s[i]=C[i]+M[i]+E[i];
				}                                   //成功储存所有数据
				for(int k=0;k<n;k++)                 //找到最大s[i]对应的i值储存在t中 
				{
					if(s[k]>maxi)
					{
						maxi=s[k];
						t=k;
					}
				}
				cout<<N[t]<<" "<<C[t]<<" "<<M[t]<<" "<<E[t];
				return 0; 
			}




114.			#include <bits/stdc++.h>
			using namespace std;
			int d(int a,int b)            //判断单科分差函数 
			{
				if((a-b)>=-5&&(a-b)<=5)
				return 0;                 //满足分差小于5则返回0 
				else 
				return 1;
			}
			int p(int a,int b)            //判断总分分差函数 
			{
				if((a-b)>=-10&&(a-b)<=10)
				return 0;                 //满足分差小于10则返回0 
				else 
				return 1;
			}
			int main()
			{
				int n,i;
				cin>>n;
				int s[n],C[n],M[n],E[n];             //分别储存总分,语文，数学，英语成绩 
				string N[n];                           //储存名字
				for(i=0;i<n;i++)
				{
					cin>>N[i]>>C[i]>>M[i]>>E[i];
					s[i]=C[i]+M[i]+E[i];
				}                                   //成功储存所有数据
				for(int j=0;j<n-1;j++)                //遍历数组寻找对手
				{
					for(int k=j+1;k<n;k++)            //不能为自己 
					{
						if((p(s[j],s[k])==0)&&(d(C[j],C[k])==0)&&(d(M[j],M[k])==0)&&(d(E[j],E[k])==0))      //判断对手 
						cout<<N[j]<<" "<<N[k]<<'\n';	
					}	
				} 
				return 0;
			}



115.			#include <bits/stdc++.h>
			using namespace std;
			struct news{
				int name;
				double a,b;               //学业成绩，素质拓展 
				double c;              //综合分数 
			}t[1000];
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>t[i].name>>t[i].a>>t[i].b;
					t[i].c=t[i].a*0.7+t[i].b*0.3;
				}
				for(int i=0;i<n;i++)
				{
					if((t[i].a+t[i].b)>140&&t[i].c>=80)
					cout<<"Excellent"<<'\n';
					else
					cout<<"Not excellent"<<'\n';
				}
				return 0;
			 } 




