			void qs(int l,int r)
			{
				int temp=a[(l+r)/2];
				int i=l,j=r;
				while(i<=j)
				{
					while(a[j]>temp)
					j--;
					while(a[i]<temp)
					i++;
					if(i<=j)
					{
						swap(a[i],a[j]);
						i++;j--;
					}
				}
				if(j>l)
				qs(l,j);
				if(i<r)
				qs(i,r);
			}
