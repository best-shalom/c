34.                   #include <iostream>
                      #include<cmath>
                      using namespace std;
                      int z(int n)
                      {
                        if (n<2)
                        return 0;
                        if (n==2)
                        return 1;
                        for (int i=2;i<=sqrt(n);i++)
                          if(n%i==0)
                          return 0;
                        return 1;
                      }
                      int main()
                      {
                        int L,s=0,n=0;
                        cin>>L;
                        for (int i=2;(s+i)<=L;i++)
                        {
                          if (z(i)==1)
                          {
                            s=s+i;
                            cout<<i<<'\n';
                            n++;
                          }
                        }
                        cout<<n;
                        return 0;
                      }





35.                   #include <iostream>
                      #include<cmath>
                      using namespace std;
                      int z(int n)
                      {
                        if (n<2)
                        return 0;
                        if (n==2)
                        return 1;
                        for (int i=2;i<=sqrt(n);i++)
                          if(n%i==0)
                          return 0;
                        return 1;
                      }
                      int h(int n)
                      {
                        int i=0,m=n;
                        while (m!=0)
                        {
                          i=i*10+m%10;
                          m=m/10;
                        }
                        if (i==n)
                        return 1;
                      }
                      int main()
                      {
                        int a,b;
                        cin>>a>>b;
                        for (int i=a;i<=b;i++)
                        {
                          if (z(i)==1&&h(i)==1)
                          cout<<i<<'\n';
                        }
                        return 0;
                      }






