                                "             ###.......######              ###            ###\n"
                                "                ...........               #...#          #...#\n"
                                "               ##*#######                 #.#.#          #.#.#\n"
                                "            ####*******######             #.#.#          #.#.#\n"
                                "           ...#***.****.*###....          #...#          #...#\n"
                                "           ....**********##.....           ###            ###\n"
                                "           ....****    *****....\n"
                                "             ####        ####\n"
                                "           ######        ######\n"
                                "##############################################################\n"
                                "#...#......#.##...#......#.##...#......#.##------------------#\n"
                                "###########################################------------------#\n"
                                "#..#....#....##..#....#....##..#....#....#####################\n"
                                "##########################################    #----------#\n"
                                "#.....#......##.....#......##.....#......#    #----------#\n"
                                "##########################################    #----------#\n"
                                "#.#..#....#..##.#..#....#..##.#..#....#..#    #----------#\n"
                                "##########################################    ############\n"
                                );
                                return 0;
                            }




4.(正确解法)                 #include <iostream>
                            using namespace std;
                            int main()
                            {
                            float i;
                            int s,a,b,c,d;
                            float A,B,C,D,t;
                            cin>>i;
                            s=i*10;
                            a=s%10;
                            b=s/10%10;
                            c=s/100%10;
                            d=s/1000%10;
                            A=a;B=(float)b/10;C=(float)c/100;D=(float)d/1000;         //前期错误：直接用B=b/10,此时，由于int b=整数，计算b/10时，强制整型，B=0，应该用（float）b来解除强制
                            t=A+B+C+D;
                            cout<<t;
                            return 0;
                            }




5.                          #include <iostream>
                            using namespace std;
                            int main()
                            {
                            int a,b,c,d,x,y;
                            cin>>a>>b>>c>>d;
                            x=c-a;
                            y=d-b;
                            if(y<0)
                            x--,y=y+60;
                            cout<<x<<" "<<y;        
                            return 0;
                            } 




6.                          #include <iostream>
                            #include<cmath>
                            using namespace std;
                            int main()
                            {
                            int T;
                            cin>>T;
                            switch(T)
                            { 
                            case 1:{cout<<"I love Luogu!";break;}
                            case 2:{cout<<"6 4";break;}
                            case 3:{cout<<"3"<<'\n'<<"12"<<'\n'<<"2";break;}
                            case 4:{cout<<"166.667";break;}
                            case 5:{cout<<"15";break;}
                            case 6:{cout<<sqrt(6*6+9*9);break;}
                            case 7:{cout<<"110\n90\n0";break;}
                            case 8:{ cout<<31.41593<<endl<<78.539825<<endl<<523.59883333333333333333333333333<<endl;break;}
                            case 9:{cout<<"22";break;}
                            case 10:{cout<<"9";break;}
                            case 11:{cout<<100.0/3;break;}
                            case 12:{cout<<"13\nR";break;}
                            case 13:{cout<<"16";break;}
                            case 14:{cout<<"50";break;}
                            } 
                            return 0;
                            }



7.                          #include <iostream>
                            using namespace std;
                            int main()
                            {
                                int m,t,s,n;
                                cin>>m>>t>>s;
                                if (t==0)
                                {
                                cout<<0;
                                return 0;
                                }
                                if (s%t==0)
                                n=m-s/t;
                                else
                                n=m-s/t-1;
                                if (n<=0)
                                cout<<0;
                                else
                                cout<<n;
                                return 0;
                            }





8.                            #include <iostream>
                              #include <bits/stdc++.h>
                              using namespace std;
                              int main()
                              {
                                  double a,b,c,s,p;
                                  cin>>a>>b>>c;
                                  p=(a+b+c)/2;
                                  s=sqrt (p*(p-a)*(p-b)*(p-c));
                                  cout<<fixed<<setprecision(1)<<s;
                                  return 0;
                              }





