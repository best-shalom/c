                                int m=n;
                                while (n!=1)
                                {	
                                  if (n%2==0)
                                  n=n/2;
                                  else
                                  n=3*n+1;
                                  i++;
                                  a[i]=n;
                                }
                                for (int j=i;j>0;j--)
                                {
                                  cout<<a[j]<<" ";
                                }
                                cout<<m;
                                return 0;
                              }



