			using namespace std;
			int main()
			{
				int n,s=1;
				cin>>n;
				for(int i=0;i<n-1;i++)               //n-1次循环
				{
					s=(s+1)*2;	
				} 
				cout<<s;
				return 0;
			}




121.			#include <bits/stdc++.h>
			using namespace std;
			struct mark{
				string name;
				int age;
				int grade;
			}a[100];
			int main()
			{
				int n;
				cin>>n;
				for(int i=0;i<n;i++)
				{
					cin>>a[i].name>>a[i].age>>a[i].grade;           //输入数据
					cout<<a[i].name<<" "<<a[i].age+1<<" ";          //一年训练
					if(a[i].grade*1.2<=600)
					cout<<a[i].grade*1.2<<endl;
					else
					cout<<600<<endl;
				}
			}




122.			#include <bits/stdc++.h>
			using namespace std;
			int m[100000];
			int main()
			{
				char a;
				for(int i=0;cin>>a&&a!='E';i++)        //碰到E就结束 
				{               
					if(a=='W')                         //把结果存到数组m 
					m[i]=1;
					else 
					m[i]=2;
				}
				int w=0,l=0;
				for(int i=0;;i++)
				{
					if(m[i]==1)
					w++;                                //胜利 
					if(m[i]==2)
					l++;                                //失败
					if(w-l>=2||l-w>=2)         
						if(w>=11||l>=11)                // 当双方比分相差大于2且一方分数大等于11输出 
						{
							cout<<w<<":"<<l<<endl;
							w=0;l=0;                     //比分清零 
						}
					if(m[i]==0)                        //读到0则记录结束，输出记录结束前的分数。 
					{
						cout<<w<<":"<<l<<endl<<endl;     //两行隔开 
						break;
					}
				}
				w=0;l=0;                                  //开始记录21分制
				for(int i=0;;i++)
				{
					if(m[i]==1)
					w++;                                //胜利 
					if(m[i]==2)
					l++;                                //失败
					if(w-l>=2||l-w>=2)          
						if(w>=21||l>=21)                // 当双方比分相差大于2且一方分数大等于21输出 
						{
							cout<<w<<":"<<l<<endl;
							w=0;l=0;                     //比分清零 
						}
					if(m[i]==0)                        //读到0则记录结束，输出记录结束前的分数。 
					{
						cout<<w<<":"<<l;           
						break;
					}
				}
				return 0; 
			}



123.			#include <bits/stdc++.h>
			using namespace std;
			int main()
			{
				int a,b;
				cin>>a>>b;               //a行b列
				char m[101][101];
				int n[101][101];
				for(int i=0;i<101;i++)
					for(int j=0;j<101;j++)
					n[i][j]=0;                      //初始化 
				for(int i=0;i<a;i++)
					for(int j=0;j<b;j++)
					{
						cin>>m[i][j];               //输入每行的地雷情况
						if(m[i][j]=='*')
						{
							n[i+1][j+1]++;
					n[i+1][j-1]++;
					n[i+1][j]++;
					n[i][j+1]++;
					n[i][j-1]++;
					n[i-1][j]++;
					n[i-1][j+1]++;
					n[i-1][j-1]++;				//相邻的八个格子都+1   
						}
					}
				//储存完毕,开始输出
				for(int i=0;i<a;i++)
				{
					for(int j=0;j<b;j++)
					{
						if(m[i][j]=='*')
						cout<<"*";
						else
						cout<<n[i][j];
					}	
					cout<<'\n';
				}
				return 0;
			}




124.			#include <bits/stdc++.h>
			using namespace std;
			struct{
				int face;
				char name[10];
			}t[100000];
			int main()
			{
				int n,m,a,b;
				cin>>n>>m;
				for(int i=0;i<n;i++)
				{
					cin>>t[i].face>>t[i].name;              //输入数据 
				}
				int x=0;
				for(int i=0;i<m;i++)
				{
					cin>>a>>b;
					if(t[x].face==1)                        //假如第x个小人朝向圈外
					{
						if(a==0)                             //圈外小人的左数b个人
						{
							for(int j=0;j<b;j++)
							{
								x++;                           //左数b次 
								if(x>=n)                        //小人序号超出，返回0
							    x=0; 
							}
						} 
						else
						{
							for(int j=0;j<b;j++)
							{
								x--;                           //右数b次 
								if(x<0)                        //小人序号超出，返回n-1
							    x=n-1; 
							}
						}
					}
					else
					{
						if(a==0)                             //圈内小人的左数b个人
						{
							for(int j=0;j<b;j++)
							{
								x--;                           //右数b次 
								if(x<0)                        //小人序号超出，返回n-1
							    x=n-1; 
							}
						} 
						else
						{
							for(int j=0;j<b;j++)
							{
								x++;                           //左数b次 
								if(x>=n)                        //小人序号超出，返回0
							    x=0; 
							}
						}
					} 
				}
				//m次操作过后
				cout<<t[x].name;
				return 0; 
			}





