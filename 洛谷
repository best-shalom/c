29.（取值太大，输入n=22，结果错误）            #include <iostream>
    （溢出）                                 using namespace std;
                                            int N(int n)
                                            {
                                              long long s=1;
                                              for (int i=1;i<=n;i++)
                                              s=s*i;
                                              return s;
                                            }
                                            int main()
                                            {
                                              long long s=0,n;
                                              cin>>n;
                                              for (int i=1;i<=n;i++)
                                              s=s+N(i);
                                              cout<<s;
                                              return 0;
                                            }


 
30.                       #include <iostream>
                          using namespace std;
                          int main()
                          {
                            int n,a,m=0;
                            cin>>n>>a;
                            for (int i=1;i<=n;i++)
                            {
                              int b=i;
                              while (b>0)
                              {
                                int c;
                                c=b%10;
                                b=b/10;
                                if (c==a)
                                m++;
                              }
                            }
                            cout<<m;
                            return 0;
                          }



